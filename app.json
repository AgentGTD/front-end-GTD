{
  "expo": {
    "name": "FlowDo",
    "slug": "FlowDo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/expo-icon2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/expo-icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#f6f8fa",
      "dark": {
        "image": "./assets/expo-icon2.png",
        "backgroundColor": "#222"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flowdo.app",
      "icon": "./assets/expo-icon2.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/expo-icon2.png",
        "backgroundColor": "#f6f8fa"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.flowdo.app",
      "versionCode": 2,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "allowBackup": false
    },
    "web": {
      "favicon": "./assets/expo-icon2.png"
    },
    "extra": {
      "eas": {
        "projectId": "503bec3b-0709-446e-bae4-5250be8e4497"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      "expo-font",
      "expo-image-picker"
    ],
    "scheme": "flowdo"
  }
}
